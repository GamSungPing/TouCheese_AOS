<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/homeConceptFragment">

    <fragment
        android:id="@+id/homeConceptFragment"
        android:name="com.example.presentation.main.view.fragment.HomeConceptFragment"
        android:label="fragment_home_concept"
        tools:layout="@layout/fragment_home_concept" >
        <action
            android:id="@+id/action_homeConceptFragment_to_testFragment"
            app:destination="@id/testFragment" />
    </fragment>
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.presentation.main.view.fragment.TestFragment"
        android:label="fragment_test"
        tools:layout="@layout/fragment_test" >
        <action
            android:id="@+id/action_testFragment_to_homeConceptFragment"
            app:destination="@id/homeConceptFragment" />
        <argument
            android:name="conceptId"
            app:argType="integer" />
    </fragment>
</navigation>